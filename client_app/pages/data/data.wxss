.title{
  width:100%;
  padding:12rpx 24rpx;
  background: white;
}
.title view{
  line-height: 80rpx;
  background: #e5e5e5;
  text-align: center;
  font-size: 32rpx;
  margin: 12rpx auto;
}
.listbox{
  background: white;
}
.list{
  width: 100%;
  padding:  24rpx;
  position: relative;
  border-top: 1rpx solid #e5e5e5;

}
.list text{
  position: absolute;
  left: 300rpx;
}
.minPic{
  width: 210rpx;
  height: 160rpx;
  display: inline-block;
  vertical-align: top;
}
.arrow{
  position: absolute;
  width: 15rpx;
  height: 29rpx;
  right: 24rpx;
  top: 50%;
  -webkit-transform:translateY(-50%);
}
.rank{
  width: 40rpx;
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  display: inline-block;
  vertical-align: top;
  background: red;
  color: white;
}
.name{
  font-weight: bold;
  top: 24rpx;
}
.EnglishName{
  top: 80rpx;
  font-size: 28rpx;
  color: #666666;
}
.type{
  bottom: 24rpx;
  font-size: 24rpx;
}





.macth{
  width: 100%;
  font-size: 28rpx;
  background: white;
  padding: 0rpx 0rpx 0 24rpx;
  height: 100rpx;
  white-space: nowrap;
  box-sizing: border-box;
  border-bottom: 2rpx solid #eeeeee;
}
.macth view{
  margin-right: 40rpx;
  line-height: 93rpx;
  display: inline-block;
  vertical-align: middle;
  box-sizing: border-box;

}
.blueColor{
  color: #396bd0;
  border-bottom: 4rpx solid #396bd0;
}
