.title{
  text-align: center;
  color: #d02828;
  line-height: 2;
}
page{
  background: white;
}
.time{
  text-align: center;
  color: #d02828;
  line-height: 2.5;
  font-size: 24rpx;
}
.picBox{
  width: 100%;
  height: 530rpx;
}
.picBox image{
  width: 100%;
  height: 550rpx;
}
.Vip{
  width: 100%;
  box-sizing: border-box;
  padding-left: 30rpx;
  line-height: 70rpx;
}
.Vip text,.Vip image{
  display: inline-block;
  vertical-align: middle;
}
.icon1{
  width: 60rpx;
  height: 44rpx;
  margin-right: 10rpx;
}
.vitBtn{
  color: #d02828;
  margin-left: 10rpx;
}

.priceList{
  width: 100%;
  padding-left: 35rpx;
 
}
.priceLi{
  width: 212rpx;
  height: 80rpx;
  border: 1rpx solid #dddddd;
  border-radius: 10rpx;
  position: relative;
  margin-top: 20rpx;
  margin-right: 20rpx;
  display: inline-block;
  vertical-align: bottom;
}
.bgColor1{
  background: #ae0606;
}
.bgColor2{
  background: #2d5ebf;
}
.bgColor3{
  background: #4dbcdb;
}
.bgColor4{
  background: #73fb53;
}
.bgColor5{
  background: #ccd952;
}
.bgColor6{
  background: #926100;
}
.bgColor7{
  background: #db5b22
}
.bgColor8{
  background: #05a55e
}
.bgColor9{
  background: #2f0c78;
}
.colorDiv{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 12rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
}
.price{
  width: 100%;
  text-align: right;
  font-size: 24rpx;
  display: block;
  padding-right: 12rpx;
  box-sizing: border-box;
  margin-top: 8rpx;
}
.VipPrice{
  width: 100%;
  text-align: right;
  font-size: 22rpx;
  color: #999999;
  display: block;
  padding-right: 12rpx;
  box-sizing: border-box;
}
.nextbtn{
  width: 680rpx;
  margin: 40rpx auto;
  background: #d02828;
  text-align: center;
  color: white;
  line-height: 80rpx;
}