/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  font-size: 32rpx;
  background: #f4f4f4;
  
}
view,text{
  box-sizing: border-box;
}
.borderTop{
  border-top: 1rpx solid #dddddd;
}
.borderBottom{
  border-top: 1rpx solid #dddddd;
}
.hidden{
  display: none;
}